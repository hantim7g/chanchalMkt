	<!-- jQuery and Bootstrap JS -->
	
	<script>
		// Toggle collapse on arrow click
		$('.arrow').click(function() {
			var section = $(this).closest('.card').find('.collapse');
			if (section.hasClass('show')) {
				section.collapse('hide');
				$(this).html('&#9660;');
			} else {
				section.collapse('show');
				$(this).html('&#9650;');
			}
		});
	</script>

